<?xml version="1.0" encoding="utf-8"?>
<elements>
	<!-- J2 Motor -->
	<element id="DID_Motor_plus_or_Winding1">
		<!--
		
		1 - maxon DC motor
		0 - maxon EC motor
		
		query with description: //sockets/motor[@description='maxon DC motor']
		
		-->
		<label resource="true" condition="//sockets/motor[@type=1]">IDS_LBL_DC_MOTOR_PLUS</label>
		<label resource="true" condition="//sockets/motor[@type=0]">IDS_LBL_EC_MOTOR_W1</label>
	</element>
	<element id="DID_Motor_minus_or_Winding2">
		<label resource="true" condition="//sockets/motor[@type=1]">IDS_LBL_DC_MOTOR_MINUS</label>
		<label resource="true" condition="//sockets/motor[@type=0]">IDS_LBL_EC_MOTOR_W2</label>
	</element>
	<element id="DID_Motor_not_connected_or_Winding3">		
		<label resource="true" condition="//sockets/motor[@type=1]">IDS_LBL_DC_MOTOR_DISCONNECTED</label>
		<label resource="true" condition="//sockets/motor[@type=0]">IDS_LBL_EC_MOTOR_W3</label>
	</element>
	<element id="DID_Motor_shield">
		<label resource="true">IDS_LBL_MOTOR_SHIELD</label>
	</element>	
	<!-- J3 -->
	<element id="DID_Hall_sensor1">
		<label resource="true" condition="//sockets/socket[@id='j3']">IDS_LBL_HALL_SENSOR1</label>
		<label resource="true">IDS_LBL_NONE</label>
	</element>
	<element id="DID_Hall_sensor2">
