		<label resource="true">IDS_LBL_NONE</label>
	</element>
	<element id="DID_plus5_VDC_Auxiliary_Output_Voltage">
		<label resource="true">IDS_LBL_PLUS5_VDC_AUX</label>
	</element>	
	<element id="DID_not_connected">
		<label resource="true">IDS_LBL_NONE</label>
	</element>	
	<element id="DID_GND">
		<label resource="true">IDS_LBL_GROUND</label>
	</element>
