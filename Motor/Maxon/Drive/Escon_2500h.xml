	</element>
	<element id="DID_Hall_GND">
		<label resource="true" condition="//sockets/socket[@id='j3']">IDS_LBL_GROUND</label>
		<label resource="true">IDS_LBL_NONE</label>
	</element>
	<!-- Common -->
	<element id="DID_plus5_VDC_Hall_Sensor_Supply_Voltage">
		<label resource="true" condition="//sockets/socket[@id='j3']">IDS_LBL_HALL_SENSOR_VDC</label>
		<label resource="true">IDS_LBL_NONE</label>
	</element>
	<element id="DID_plus5_VDC_Encoder_Supply_Voltage">
